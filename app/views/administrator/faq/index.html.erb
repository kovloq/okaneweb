<div class="page-title-breadcrumb">
	<div class="page-header pull-left">
		<div class="page-title">FAQ</div>
	</div>
	<ol class="breadcrumb page-breadcrumb hidden-xs">
        <li><i class="fa fa-home"></i>&nbsp;
        	<%= link_to "Home", :controller => "administrator/home", :action => "index" %>
        	&nbsp;&nbsp;<i class="fa fa-angle-right"></i>&nbsp;&nbsp;</li>
        <li class="active">FAQ</li>
        <!-- <li class="active">Invoice</li> -->
    </ol>
</div>



<div class="box-content">
    <div class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="panel">
                	<div class="panel-body">
                    <%# daftar transaksi %>
                    <%= link_to "Add",{:controller=>"administrator/faq",:action=>"new"},:class=>"btn btn-primary" %>
                    <br/>
                    <br/>

                    <table class="table table-bordered">
                        <tr>
                            <th>Title</th>
                            <th>Order</th>
                            <th>Edit</th>
                            <th>Delete</th>
                        </tr>
                        
                        <% @faq.each do |t| %>
                            <tr>
                                <td><%= t.title %></td>
                                <td><%= t.order %></td>
                                
                                <td>
                                    <%= link_to "Edit",{:controller=>"administrator/faq",:action=>"edit",:id=>t.id},:class=>"btn btn-warning" %>

                                    
                                </td>
                                <td><%= link_to "Delete",{ :controller=>"administrator/faq",:action=>"destroy", :id=> t.id }, {:method => :delete,:class=>"btn btn-danger",:data => {:confirm => 'Are you sure?'}} %></td>
                            </tr>
                        <% end %>
                    </table>
                    </div>
                    <div class="panel-footer">
                        <%= paginate @faq %>
                    </div>
				</div>
			</div>
		</div>
	</div>
</div>